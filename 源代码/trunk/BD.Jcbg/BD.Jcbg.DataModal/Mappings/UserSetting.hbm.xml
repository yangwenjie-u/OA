<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="BD.Jcbg.DataModal.Entities.UserSetting, BD.Jcbg.DataModal" table="UserSetting" lazy="false">
		<id name="RECID" type="Int32" unsaved-value="null">
			<column name="RECID" length="4" sql-type="int" not-null="true" unique="true"/>
			<generator class="native" />
		</id>
		<property name="UserName" type="String">
			<column name="UserName" length="50" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="SettingId" type="String">
			<column name="SettingId" length="200" sql-type="nvarchar" not-null="false"/>
		</property>
		<property name="SettingValue" type="String">
			<column name="SettingValue" length="1000" sql-type="nvarchar" not-null="false"/>
		</property>
	</class>
</hibernate-mapping>
