<script>
    var that
    new Vue({
        el: '#app',
        data: {
            showApp: false,
            f: {},
            ccqkList: [{
                    label: '----',
                    value: '----'
                }, {
                    label: '与图纸相一致',
                    value: '与图纸相一致'
                },
                {
                    label: '发现问题（详见联系单）',
                    value: '发现问题（详见联系单）'
                }
            ],
            pdjlList: [{
                    label: '----',
                    value: '----'
                }, {
                    label: '合格',
                    value: '合格'
                },
                {
                    label: '整改合格',
                    value: '整改合格'
                }
            ],
            baseData: {}
        },
        methods: {
            changeCCBW: function (val, index) {
                if (val == '----') {
                    this.f['ccsl' + index] = '----'
                    this.f['ccqk' + index] = '----'
                    this.f['pdjl' + index] = '----'
                }
            },
            submit: function () {
                console.log(JSON.parse(JSON.stringify(that.f)))
                var obj = JSON.parse(JSON.stringify(that.f))
                // obj['recid'] = GetQueryString('recid')
                // obj['parentid'] = GetQueryString('parentid')
                // obj['fxbaid'] = GetQueryString('fxbaid')

                obj['xfyspd'] = ""
                $.ajax({
                    url: "/wlxf/addXfyspd",
                    type: 'post',
                    data: {
                        recid: GetQueryString('recid'),
                        parentid: GetQueryString('parentid'),
                        fxbaid: GetQueryString('fxbaid'),
                        serialno: GetQueryString('serialno'),
                        xfyspd: JSON.stringify(obj)
                    },
                    dataType: "json",
                    success: function (res) {
                        if (res.code == 0) {
                            //that.$message.success('保存成功')
                            alert('保存成功');
                            parent.layer.closeAll();
                        } else
                        {
                            alert(res.msg);
                        }
                    }
                })
            },
            getBaseData: function () {
                $.ajax({
                    url: "/wlxf/xcpdgcxx",
                    type: 'post',
                    data: {
                        serialno: GetQueryString('serialno'),
                        fxbaid: GetQueryString('fxbaid'),
                        recid: GetQueryString('recid'),
                        parentid: GetQueryString('parentid'),
                    },
                    dataType: "json",
                    async: false,
                    success: function (res) {
                        console.log(res.rows.xcpdxx);
                        if (res.rows.xcpdxx.length!=1) {
                            var d = res.rows.xcpdxx
                            d.forEach(function (e) {
                                that.f[e.itemname] = e.itemvalue
                            })
                        } else {
                            that.f['xcpdbh'] = res.rows.xcpdbh
                            that.f['gcmc'] = res.rows.xcpdxx[0].gcmc
                            that.f['gcdd'] = res.rows.xcpdxx[0].gcdd
                            that.f['xcpdry'] = res.rows.xcpdxx[0].xcpdry
                            that.f['gcmc'] = res.rows.xcpdxx[0].gcmc
                        }
                    }
                })
            }
        },
        mounted: function () {
            that = this
            that.showApp = true
            var f = {
                xcpdbh: '',
                gcmc: '',
                gcdd: '',
                xcpdry: '',
                xcpdsj: new Date(),
                xcpdjl: '',
            }
            for (var i = 1; i < 116; i++) {
                f['ccbw' + i] = ''
                f['ccsl' + i] = '2'
                f['ccqk' + i] = '与图纸相一致'
                f['pdjl' + i] = '合格'
            }
            var qsjcList = [1, 4, 5, 6, 7, 8, 19, 33, 34, 35, 36, 37, 38, 44, 45, 46, 47, 48, 54, 55, 56,
                57, 61, 62, 63, 64, 65
            ]
            qsjcList.forEach(function (e) {
                f['ccsl' + e] = '全数检查'
            })
            console.log(JSON.parse(JSON.stringify(f)))
            that.f = f
            that.getBaseData()
        }
    })

    function GetQueryString(name) {
        var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
        var r = window.location.search.substr(1).match(reg);
        if (r != null) return unescape(r[2]);
        return "";
    }
</script>
