@using BD.Jcbg.Common;
<!DOCTYPE html>

<html>
<head>
    <!--移动设备-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!--高速模式,webkit内核-->
    <meta name="renderer" content="webkit">
    <!--不支持IE兼容模式-->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!--禁止搜索引擎转码,并附带广告-->
    <meta http-equiv="Cache-Control" content="no-transform" />
    <meta http-equiv="Cache-Control" content="no-siteapp" />

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>@Configs.Title</title>
    <link href="@SkinManager.GetBootstrapPath("/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@SkinManager.GetCssPath("uploadify.css")" rel="stylesheet" type="text/css" />
    <link href="@SkinManager.GetCssPath("fileupload.css")" rel="stylesheet" type="text/css" />
    <script src="@SkinManager.GetBootstrapPath("/js/jquery.min.js")" type="text/javascript" charset="utf-8"></script>
    <script src="@SkinManager.GetBootstrapPath("/js/bootstrap.min.js")" type="text/javascript" charset="utf-8"></script>
    <script src=@SkinManager.GetBootstrapPath("/PlugIns/Layer/layer.js") type="text/javascript" charset="utf-8"></script>
    <script src="@SkinManager.GetJsPath("jquery.uploadify.min.js")" type="text/javascript" charset="utf-8"></script>
    <script src="@SkinManager.GetJsPath("fileupload.js")" type="text/javascript" charset="utf-8"></script>

    @RenderSection("head", false)


</head>
<body>
    @RenderBody()
</body>
</html>
