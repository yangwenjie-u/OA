@using BD.Jcbg.Common;
@{
    Layout = "~/Views/jc/_tablayout.cshtml";
}
@section head
{
    <script language="javascript" type="text/javascript">

        $(function () {
            try {
                var rdm = Math.random();
                var url = "/doexcel/uploadFile?&_=" + rdm;
                parent.layer.open({
                    type: 2,
                    title: '更新银行账户余额',
                    shadeClose: true,
                    shade: 0.8,
                    area: ['30%', '30%'],
                    content: url,
                    end: function () { 
                        var existsIndex = window.parent.TabControlGetIndex('YHZH_YEGX'); //关闭一级菜单
                        window.parent.TabControlRemove(existsIndex);
                    }
                });
            } catch (e) {
                alert(e);
            }

        });
    </script>
}
