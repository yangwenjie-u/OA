<!DOCTYPE html>
<html>
<head>
    <!--移动设备-->
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<!--高速模式,webkit内核-->
<meta name="renderer" content="webkit">
<!--不支持IE兼容模式-->
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<!--禁止搜索引擎转码,并附带广告-->
<meta http-equiv="Cache-Control" content="no-transform" />
<meta http-equiv="Cache-Control" content="no-siteapp" />
<!--如果为IE9以下,就自动跳转到提示页面-->
<!--[if lt IE 9]>
<meta http-equiv="Refresh" content="0;url=http://www.bing.com">
<![endif]-->
<link href="~/Contents/Css/bootstrap.css" rel="stylesheet" />
<link href="~/Contents/Css/font-awesome.css" rel="stylesheet" />
<link href="~/Contents/Css/animate.css" rel="stylesheet" />
<link href="~/Contents/Css/style.css" rel="stylesheet" />
<script src="~/Contents/Js/jquery.js"></script>
<script src="~/Contents/js/bootstrap.js"></script>
<script src="~/Contents/PlugIns/Layer/layer.js"></script>
<script src="~/Contents/Js/sea.js"></script>
    <title>@ViewBag.Title</title>
</head>
<body class="fixed-sidebar full-height-layout gray-bg">
    @RenderBody()
</body>
</html>
