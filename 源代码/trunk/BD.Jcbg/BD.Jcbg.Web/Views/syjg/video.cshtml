@{
    Layout = "~/Views/User/_dialoglayout.cshtml";
}


<script language="javascript" src="/skins/default/js/player/jwplayer.js" type="text/javascript"></script>
<script type="text/javascript">
	var ctrlWidth = @ViewData["CtrlWidth"];
	if (ctrlWidth == 0)
		ctrlWidth = $(window).width()-10;
	var ctrlHeight = @ViewData["CtrlHeight"];
	if (ctrlHeight == 0)
		ctrlHeight = $(window).height()-10;  
	var filePath = '@ViewData["FileName"]';

	$(function () {
        jwplayer("player").setup({
            flashplayer:'/skins/default/js/player/player.swf',
            file:filePath,
            height:ctrlHeight,
            width:ctrlWidth
        });
        jwplayer("player").onReady(function(){
            jwplayer("player").play();
        });

	});
</script>
<div id="player">请启用视频播放器</div>