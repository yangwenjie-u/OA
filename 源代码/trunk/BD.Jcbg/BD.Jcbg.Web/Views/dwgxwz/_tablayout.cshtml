@using BD.Jcbg.Common;
<!DOCTYPE html>

<html>
<head>
    <!--移动设备-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!--高速模式,webkit内核-->
    <meta name="renderer" content="webkit">
    <!--不支持IE兼容模式-->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!--禁止搜索引擎转码,并附带广告-->
    <meta http-equiv="Cache-Control" content="no-transform" />
    <meta http-equiv="Cache-Control" content="no-siteapp" />
    <!--如果为IE9以下,就自动跳转到提示页面-->
    <!--[if lt IE 9]>
    <meta http-equiv="Refresh" content="0;url=http://www.bing.com">
    <![endif]-->
    <title></title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link href="@SkinManager.GetBootstrapPath("/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@SkinManager.GetDatePath("/css/bootstrap.css")" rel="stylesheet" type="text/css" />
    <link href="@SkinManager.GetCssPath("xiaozhong_css.css")" rel="stylesheet" type="text/css" />
    <script src=@SkinManager.GetBootstrapPath("/PlugIns/combobox/css/bootstrap-combobox.css") type="text/javascript" charset="utf-8"></script>
    <script src="@SkinManager.GetBootstrapPath("/js/jquery.min.js")" type="text/javascript" charset="utf-8"></script>
    <script src="@SkinManager.GetBootstrapPath("/js/bootstrap.min.js")" type="text/javascript" charset="utf-8"></script>
    <script src=@SkinManager.GetBootstrapPath("/PlugIns/Layer/layer.js") type="text/javascript" charset="utf-8"></script>
    <script src=@SkinManager.GetBootstrapPath("/PlugIns/combobox/js/bootstrap-combobox.js") type="text/javascript" charset="utf-8"></script>
    <script src=@SkinManager.GetDatePath("/js/zebra_datepicker.js") type="text/javascript" charset="utf-8"></script>
    <script src=@SkinManager.GetDatePath("/js/core.js") type="text/javascript" charset="utf-8"></script>
    @RenderSection("head", false)
</head>
<body>
    @RenderBody()
</body>
</html>